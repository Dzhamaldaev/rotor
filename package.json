{
  "name": "rotor",
  "version": "1.0.0",
  "description": "Rotor",
  "private": true,
  "scripts": {
    "dev": "npm run development && rm mix-manifest.json",
    "development": "NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "npm run development -- --watch",
    "hot": "NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js && rm mix-manifest.json",
    "prod": "npm run production",
    "production": "NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js && rm mix-manifest.json"
  },
  "dependencies": {
    "laravel-mix": "^2.1.11",
    "url-parse": ">=1.4.3",
    "cryptiles": ">=4.1.2"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/visavi/rotor.git"
  },
  "author": "Alexander Grigorev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/visavi/rotor/issues"
  },
  "homepage": "https://github.com/visavi/rotor#readme",
  "main": "webpack.mix.js",
  "directories": {
    "test": "tests"
  }
}
