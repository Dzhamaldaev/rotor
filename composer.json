{
    "name": "visavi/rotor",
    "description": "RotorCMS",
    "keywords": ["cms", "wap", "rotor"],
    "homepage": "http://visavi.net",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Alexander Grigorev",
            "email": "admin@visavi.net",
            "homepage": "http://visavi.net"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "altorouter/altorouter": "~1.2",
        "cbschuld/browser.php": "dev-master",
        "gregwar/captcha": "^1.1",
        "illuminate/http": "~5.4",
        "illuminate/translation": "~5.4",
        "mobiledetect/mobiledetectlib": "^2.8",
        "pclzip/pclzip": "^2.8",
        "php-ffmpeg/php-ffmpeg": "~0.6",
        "swiftmailer/swiftmailer": "~5.4",
        "vlucas/phpdotenv": "^2.4",
        "robmorgan/phinx": "^0.6.5",
        "verot/class.upload.php": "^0.33.0",
        "php-curl-class/php-curl-class": "^7.1",
        "jenssegers/blade": "^1.1",
        "visavi/cleanup": "^1.0",
        "surt/granada": "dev-master",
        "illuminate/database": "^5.4"
    },
    "require-dev": {
        "filp/whoops": "^2.1"
    },
    "scripts": {
        "post-create-project-cmd": [
            "App::install",
            "php -r \"copy('.env.example', '.env');\""
        ]
    },
    "autoload": {
        "psr-4": {
            "": [
                "app/classes/",
                "app/models/"
            ]
        },
        "files": [
            "app/routes.php",
            "app/helpers.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "7.0-dev"
        }
    }
